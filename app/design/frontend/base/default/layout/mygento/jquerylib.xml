<layout version="0.1.0">
    <default>
        <reference name="head">
            <action method="addJs" ifconfig="jquerylib/general/enabled">
                <script>jquerylib/jquery.min.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/general/legacy">
                <script>jquerylib/jquery.legacy.min.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/general/migrate">
                <script>jquerylib/jquery.migrate.min.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/general/enabled">
                <script>jquerylib/mygento/jquery.init.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/general/legacy">
                <script>jquerylib/mygento/jquery.init.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/plugins/easing">
                <script>jquerylib/jquery.easing.min.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/plugins/carousel">
                <script>jquerylib/jquery.jcarousel.min.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/plugins/ui">
                <script>jquerylib/jquery-ui/jquery-ui.min.js</script>
            </action>

            <action method="addItem" ifconfig="jquerylib/plugins/ui">
                <type>js_css</type>
                <name>jquerylib/jquery-ui/jquery-ui.min.css</name>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/hovercard">
                <script>jquerylib/jquery.hovercard.min.js</script>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/ddslick">
                <script>jquerylib/jquery.ddslick.min.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/plugins/masked">
                <script>jquerylib/jquery.maskedinput.min.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/plugins/backstretch">
                <script>jquerylib/jquery.backstretch.min.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/plugins/modernizr">
                <script>jquerylib/modernizr.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/plugins/chosen">
                <script>jquerylib/chosen/chosen.jquery.min.js</script>
            </action>
            <action method="addItem" ifconfig="jquerylib/plugins/chosen">
                <type>js_css</type>
                <name>jquerylib/chosen/chosen.css</name>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/galleria">
                <script>jquerylib/galleria/galleria.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/plugins/galleria">
                <script>jquerylib/mygento/galleria.loadtheme.js</script>
            </action>
            <action method="addItem" ifconfig="jquerylib/plugins/galleria">
                <type>js_css</type>
                <name>jquerylib/galleria/themes/classic/galleria.classic.css</name>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/fancy">
                <script>jquerylib/fancybox/jquery.fancybox.pack.js</script>
            </action>
            <action method="addItem" ifconfig="jquerylib/plugins/fancy">
                <type>js_css</type>
                <name>jquerylib/fancybox/jquery.fancybox.css</name>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/skitter">
                <script>jquerylib/skitter/js/jquery.skitter.min.js</script>
            </action>

            <action method="addItem" ifconfig="jquerylib/plugins/skitter">
                <type>js_css</type>
                <name>jquerylib/skitter/css/skitter.styles.min.css</name>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/validation">
                <script>jquerylib/jquery.validate.min.js</script>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/sweetalert">
                <script>jquerylib/sweetalert/sweetalert.min.js</script>
            </action>
            <action method="addItem" ifconfig="jquerylib/plugins/sweetalert">
                <type>js_css</type>
                <name>jquerylib/sweetalert/sweetalert.css</name>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/sweetalert">
                <script>jquerylib/jquery.mousewheel.min.js</script>
            </action>
            <action method="addJs" ifconfig="jquerylib/plugins/jscrollpane">
                <script>jquerylib/jquery.jscrollpane.min.js</script>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/owl">
                <script>jquerylib/owl.carousel.min.js</script>
            </action>
            <action method="addItem" ifconfig="jquerylib/plugins/owl">
                <type>js_css</type>
                <name>jquerylib/owl.carousel.min.css</name>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/slick">
                <script>jquerylib/slick/slick.min.js</script>
            </action>
            <action method="addItem" ifconfig="jquerylib/plugins/slick">
                <type>js_css</type>
                <name>jquerylib/slick/slick.css</name>
            </action>
            <action method="addItem" ifconfig="jquerylib/plugins/slick">
                <type>js_css</type>
                <name>jquerylib/slick/slick-theme.css</name>
            </action>

            <action method="addJs" ifconfig="jquerylib/plugins/pace">
                <script>jquerylib/pace/pace.min.js</script>
            </action>
        </reference>
    </default>
</layout>
